<div class="tile-match-featured">
  <div class="fighter-list">
    {{#with fighterOne}}
      <div class="fighter first">
        <div class="fighter-name">
          {{person.name}}
        </div> 
        <div class="character-list">
        {{#each characters}}
          <span class="character-name">{{name}}</span> 
        {{/each}}
        </div>
      </div> 
    {{/with}}
    
    <div class="vs">VS</div>

    {{#with fighterTwo}}
      <div class="fighter second">
        <div class="fighter-name">
          {{person.name}}
        </div> 
        <div class="character-list">
        {{#each characters}}
          <span class="character-name">{{name}}</span> 
        {{/each}}
        </div>
      </div> 
    {{/with}}
  </div>
  <div class="description">
    gon' make your panties drop like skrillex
  </div>
</div>
