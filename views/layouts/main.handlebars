<body>
  <header id="header">
    <div class="container">
      <div class="page-header">
        <div class="page-brand">
          <a href="/">fight casts</a> 
        </div>
        <div class="tab active">
          <div class="inner-tab sf4">
          </div>
        </div>
        <div class="tab">
          <div class="inner-tab mvc3">
          </div>
        </div>
        <div class="tab">
          <div class="inner-tab sfxt">
          </div>
        </div>
        <ul class="links">
          <li>
            <span class="link-text" id="signup">signup</span>
          </li>
          <li>
            <span class="link-text" id="login">login</span>
          </li>
          <li>
            <span class="link-text" id="account">account</span>
          </li>
          <li>
            <span class="link-text" id="forgotPassword">forgot password</span>
          </li>
        </ul>
        <div class="header-form">
          <form id="userForm" class="stacked-form">
            <input class="input-text" 
              name="email" 
              placeholder="email"/> 
            <input type="password" 
              class="input-text" 
              name="password" 
              placeholder="password"/> 
          </form>
        </div>
      </div>
    </div>
  </header>

  <header id="subheader">
    <div class="container">
      <div class="page-subheader">
        <div class="search-bar" id="searchBar">
          {{> searchBar}}      
        </div>    
        <div class="filters">
          {{> pastMatches}}      
        </div>
      </div>
    </div>
  </header>

  <div id="main-content">
    <div class="container">
      {{{body}}}
    </div>
  </div>

  <footer id="subfooter">
    <div class="container">
      <div class="page-subfooter">
      </div>
    </div>
  </footer>

  <footer id="footer">
    <div class="container">
      <div class="page-footer">
      
      </div> 
    </div> 
