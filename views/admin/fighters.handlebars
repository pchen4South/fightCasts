<div class="col-md-2">
  {{> adminlist}}
</div>
<div class="col-md-4">
  {{> forms/createFighter}}
</div>
<div class = "col-md-6">
  <form role = "form" method = "POST"> 
    <ul style="list-style-type:none">
      {{#each fighters}} 
      <div class = "col-md-6">
        <li>
        <button formaction = "/admin/fighters/{{id}}/delete"
          type = "submit"
          class = "btn btn-xs btn-default" 
          name = "id" value = {{id}}> X </button>
        <img src = "{{countryIcon person.country}}"/>
        <b>{{person.name}}</b>
        as 
        {{#each characters}}
          <img src = "{{gameIcon game}}"/>
          <b>{{name}} </b>
        {{/each}}
        </li>
      </div>
      {{/each}}
    </ul>
  </form>
</div>
