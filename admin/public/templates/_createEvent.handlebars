<div class = "adminFormWrapper">
  <form role="form">
    <h3>Create Event</h3>
    <div class="form-group">
      <label for="name">Name</label>
      {{input type="text" 
        disabled=inFlight 
        class = "form-control"
        value=data.name placeholder = "Event Name"}}
    </div> 
    <button class="btn btn-info" {{action "onSubmit" data "event"}}>
      create!
    </button> 
  </form> 
</div>